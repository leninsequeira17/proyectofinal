package Pedro;
public class Zapacalculo implements operazapateria{
	 
	 @Override
	  public int Suma(int p, int q){
	     return p+q;
	  }

	 @Override
	  public int Resta(int p, int q){
	    return p-q;
	  }

	 @Override
	  public int Multiplicacion(int p, int q){
	       return p*q;
	   }

	 @Override
	  public double Division(int p, int q){
	     return p/q;
	  }

	 }

